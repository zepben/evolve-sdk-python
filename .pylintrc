[MASTER]
init-hook='import sys; sys.path.append("./src/"); sys.path.append("./test"); '
[ZEPBEN]
max-line-length=160
variable-rgx=[a-z_][a-z0-9_]{0,30}$
argument-rgx=[a-z_][a-z0-9_]{0,30}$
disable=no-else-return,too-few-public-methods,missing-module-docstring
no-docstring-rgx=^_|^test_|^Test
max-args=10
